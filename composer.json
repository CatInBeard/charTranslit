{
    "name": "catinbeard/chartranslit",
    "description": "Library for transliterating text from one language to another",
    "type": "library",
    "homepage": "https://github.com/CatInBeard/charTranslit",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Catinbeard\\Chartranslit\\": "src/"
        },
        "files":[
            "src/Transliteration.php",
            "src/ruToEn.php",
            "src/enToRu.php",
            "src/ruToEnSym.php",
            "src/enToRuSym.php",
            "src/deToEn.php",
            "src/enToDe.php",
            "src/deToEnSym.php",
            "src/enToDeSym.php",
            "src/grToEn.php",
            "src/enToGr.php",
            "src/grToEnSym.php",
            "src/enToGrSym.php"
        ]
    },
    "autoload-dev":{
        "psr-4": {
            "Catinbeard\\Chartranslit\\Tests": "src/"
        },
        "files":[
            "tests/TestRu.php",
            "tests/TestDe.php",
            "tests/TestGr.php",
            "TestTransliterator.php"
        ]
    },
    "authors": [
        {
            "name": "Grigoriy Efimov",
            "email": "efimov-gm@newpage.xyz",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "phpstan/phpstan": "1.7.x-dev",
        "squizlabs/php_codesniffer": "4.0.x-dev"
    }
}
